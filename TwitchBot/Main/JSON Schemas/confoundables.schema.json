{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "Description": {
        "type": "string"
      },
      "Rarity": {
        "type": "integer",
        "minimum": 1,
        "maximum": 16
      },
      "BaseWinRate": {
        "type": "number",
        "minimum": 0,
        "maximum": 1
      },
      "Spell": {
        "type": "string",
        "enum": [
          "accio", "aguamenti", "alohomora",
          "arrestomomentum", "bombarda", "combatbolt",
          "diffindo", "expectopatronum", "finite",
          "flipendo", "incendio", "riddikkulus"
        ]
      }
    }
  }
}